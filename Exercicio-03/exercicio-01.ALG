Algoritmo "semnome"
Var

   nome: caracter
   idade: inteiro
   nota: real

Inicio

      EscrevaL("Qual seu nome: ?")
      Leia(nome)
      EscrevaL("Qual sua idade: ?")
      Leia(idade)
      EscrevaL("Qual sua nota: ?")
      Leia(nota)
      
EscrevaL("Meu nome é: ", nome, " tenho", idade, "anos de idade e minha nota é: ",nota)

Fimalgoritmo